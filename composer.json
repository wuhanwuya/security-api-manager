{
  "name": "hso/test-api",
  "description": "api安全链路追踪",
  "license": "MIT",
  "authors": [
    {
      "name": "hanshuo",
      "email": "hanshuo@dxy.cn"
    }
  ],
  "require": {
    "php": ">=7.1.1",
    "laravel/framework": ">=6.0",
    "guzzlehttp/guzzle": ">=6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7|^8",
    "orchestra/testbench": "^5.0|^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Hso\\TestApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hso\\TestApi\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hso\\TestApi\\SecurityApiMangerServiceProvider"
      ]
    }
  }
}